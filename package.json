{
  "name": "@l-garavno/c-connector",
  "version": "0.0.10",
  "description": "Connector for C-Connector",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist ./coverage ./node_modules",
    "build": "rm -rf ./dist && tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@types/pg": "^8.11.14",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/l-garavno/c-connector"
  },
  "bugs": {
    "url": "https://github.com/l-garavno/c-connector/issues"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "files": [
    "dist",
    "LICENSE"
  ],
  "author": "L-Garavno",
  "license": "MIT",
  "homepage": "https://github.com/l-garavno/c-connector#readme",
  "dependencies": {
    "googleapis": "^148.0.0",
    "pg": "^8.15.6"
  }
}
